{
  "apiVersion": "v1",
  "spec": {
    "kind": "Service",
    "spec": {
      "type": "deployment",
      "deployment": {
        "docker": {
          "file": "Dockerfile"
        }
      },
      "ports": [
        {
          "name": "http",
          "internalPort": 8080,
          "protocol": "HTTP"
        }
      ],
      "healthChecks": [
        {
          "path": "/health",
          "port": 8080,
          "protocol": "HTTP",
          "initialDelaySeconds": 30,
          "periodSeconds": 30
        }
      ],
      "runtimeEnvironment": {}
    }
  }
}
